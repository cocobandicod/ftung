RewriteEngine On
Options All -Indexes

RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

RewriteRule ^$ index.php?id=home [L]
RewriteRule ^index.html$ index.php?id=home [L]
RewriteRule ^login.html$ login.php?id=home [L]

RewriteRule ^([a-zA-Z-]+).html$ detail.php?judul=$1 [L]
RewriteRule ^([a-zA-Z-/]+)/prodi/([a-zA-Z-]+).html$ detail.php?judul=$1&get=prodi&prodi=$2 [L]

RewriteRule ^search/?$ pencarian.php?get=$1 [L,QSA]